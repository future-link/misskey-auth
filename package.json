{
  "name": "misskey-auth",
  "version": "1.0.0",
  "description": "OAuth2 authz server for misskey-delta",
  "main": "dest/index.js",
  "scripts": {
    "start": "node dest/index.js",
    "build": "run-p build:*",
    "build:ts": "tsc",
    "lint": "run-p lint:*",
    "lint:ts": "tslint -p .",
    "watch": "run-s build watch-run",
    "watch-run": "run-p watch:*",
    "watch:start": "node-dev dest/index.js",
    "watch:ts": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tosuke/misskey-auth.git"
  },
  "keywords": [
    "oauth2"
  ],
  "author": "tosuke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tosuke/misskey-auth/issues"
  },
  "homepage": "https://github.com/Tosuke/misskey-auth#readme",
  "dependencies": {
    "jsonwebtoken": "^7.4.3",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.2.0",
    "koa-pug": "^3.0.0-2",
    "koa-router": "^7.1.1",
    "log4js": "^1.1.1",
    "mongoose": "^4.9.7",
    "pem-jwk": "^1.5.1",
    "pug": "^2.0.0-beta.12",
    "redis": "^2.7.1",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^7.2.3",
    "@types/koa": "^2.0.39",
    "@types/koa-bodyparser": "^3.0.23",
    "@types/koa-pug": "^3.0.2",
    "@types/koa-router": "^7.0.22",
    "@types/log4js": "^0.0.32",
    "@types/mongoose": "^4.7.11",
    "@types/redis": "^2.6.0",
    "@types/request-promise-native": "^1.0.5",
    "node-dev": "^3.1.3",
    "npm-run-all": "^4.0.2",
    "tslint": "^5.1.0",
    "typescript": "^2.3.2"
  }
}
